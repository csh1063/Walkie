import SwiftUI
import SwiftData

@main
struct {{name}}App: App {

	@Environment(\.scenePhase) var scenePhase
    @UIApplicationDelegateAdaptor private var appDelegate: AppDelegate

	init () { 
		print("app init")
	}

    var body: some Scene {
        WindowGroup {
            MainView()
                .onChange(of: scenePhase) { phase in    // 화면 phase
					switch phase { 
					case .active: 
						print("app active")
					case .inactive:
						print("app inactive")
					case .background:
						print("app background")
                    @unknown default:
                        print("app unknown default")
					}
				}
				.onOpenURL { url in       // 딥링크
					print("app URL: \(url)")
				}
				// .onContinueUserActivity("<이름>") { userActivity in     // 푸시 등으로 앱으로 진입할 때에 
				// 	if let things = userActivity.userInfo?["something"] as? String {
				// 		print("app get \(things)")
				// 	}
				// }
        }
    }
}
